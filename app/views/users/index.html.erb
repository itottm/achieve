<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <%= profile_img(user) %>
        <p><%= link_to user.name, user_path(user) %></p>
        <p><%= user.email %></p>

        <% unless user.id === current_user.id %>
          <p><%= render 'follow_form', user: user %></p>
        <% end %>
      </div>
  <% end %>
</div>